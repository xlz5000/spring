<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="book">


	<select id="booklist" resultType="com.ict.edu02.vo.BookVO">
	
		select * from book
	
	</select>
	
	<select id="bookdetail" parameterType="com.ict.edu02.vo.BookVO" resultType="com.ict.edu02.vo.BookVO">
		select * from book where bookid = #{bookid}
		
		
	</select>



	<delete id="bookdelete" parameterType="com.ict.edu02.vo.BookVO">
	 	delete from book where bookid = #{bookid}
	
	
	
	</delete>
</mapper>